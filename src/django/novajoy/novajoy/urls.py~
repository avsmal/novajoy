from django.conf.urls import patterns, include, url
from Server.views import mainPage,testHtml,helloPage
# Uncomment the next two lines to enable the admin:
from django.contrib import admin
from django.contrib.auth.views import login,logout
admin.autodiscover()

urlpatterns = patterns('',
    # Examples:
    # url(r'^$', 'novajoy.views.home', name='home'),
    # url(r'^novajoy/', include('novajoy.foo.urls')),

    # Uncomment the admin/doc line below to enable admin documentation:
    # url(r'^admin/doc/', include('django.contrib.admindocs.urls')),

    # Uncomment the next line to enable the admin:
    #url(r'^site_media/(?P<path>.*)$','django.views.static.serve',
    #    {'document_root':'/home/vovan/novajoy/novajoy/media/'}),
    url(r'^login/$',login),
    url(r'^logout$',logout),
    url(r'^admin/', include(admin.site.urls)),
    url(r'^test/',mainPage),
    url(r'^testHtml/',testHtml),
    url(r'^helloPage/',helloPage),
)
